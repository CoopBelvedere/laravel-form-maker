{
    "name": "coopbelvedere/laravel-form-maker",
    "description": "Modular api for creating forms in Laravel",
    "keywords": ["laravel", "form maker", "form builder", "form generator"],
    "type": "library",
    "support": {
        "issues": "https://github.com/CoopBelvedere/laravel-form-maker/issues",
        "source": "https://github.com/CoopBelvedere/laravel-form-maker"
    },
    "require": {
        "php": ">=7.2.0",
        "ext-fileinfo": "*",
        "laravel/framework": "5.7.*|5.8.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Boucher",
            "email": "pascalboocher@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Belvedere\\FormMaker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Belvedere\\FormMaker\\FormMakerServiceProvider",
                "Belvedere\\FormMaker\\Http\\Resources\\Input\\InputResourceServiceProvider",
                "Belvedere\\FormMaker\\Http\\Resources\\Sibling\\SiblingResourceServiceProvider",
                "Belvedere\\FormMaker\\Models\\Rankings\\RankingServiceProvider",
                "Belvedere\\FormMaker\\Models\\HtmlAttributes\\HtmlAttributeServiceProvider",
                "Belvedere\\FormMaker\\Models\\Rules\\RuleServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Checkbox\\CheckboxServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Color\\ColorServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Datalist\\DatalistServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Date\\DateServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Email\\EmailServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\File\\FileServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Image\\ImageServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Month\\MonthServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Number\\NumberServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Option\\OptionServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Password\\PasswordServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Radio\\RadioServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Range\\RangeServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Search\\SearchServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Select\\SelectServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Text\\TextServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Textarea\\TextareaServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Time\\TimeServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Url\\UrlServiceProvider",
                "Belvedere\\FormMaker\\Models\\Inputs\\Week\\WeekServiceProvider",
                "Belvedere\\FormMaker\\Models\\Siblings\\Label\\LabelServiceProvider"
            ]
        }
    }
}
