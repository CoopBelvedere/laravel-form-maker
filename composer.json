{
    "name": "coopbelvedere/laravel-form-maker",
    "description": "Modular api for creating forms in Laravel",
    "keywords": ["laravel", "form maker", "form builder", "form generator"],
    "type": "library",
    "support": {
        "issues": "https://github.com/CoopBelvedere/laravel-form-maker/issues",
        "source": "https://github.com/CoopBelvedere/laravel-form-maker"
    },
    "require": {
        "php": ">=7.2.0",
        "ext-fileinfo": "*",
        "laravel/framework": "5.7.*|5.8.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Boucher",
            "email": "pascalboocher@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Belvedere\\FormMaker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Belvedere\\FormMaker\\FormMakerServiceProvider",
                "Belvedere\\FormMaker\\Models\\Ranking\\RankingServiceProvider",
                "Belvedere\\FormMaker\\Models\\HtmlAttributes\\HtmlAttributeServiceProvider"
            ]
        }
    }
}
